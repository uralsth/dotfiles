# Basic binds
super + d
      dmenu_run -p 'Run:'
super + p
      mytools
super + r
      dm-websearch
super + q
      firefox
super + shift + f
      firefox -private-window
super + control + q
      qutebrowser
super + e
      emacsclient -c -a '/usr/bin/emacs'
super + s
      LD_PRELOAD=/usr/lib/spotify-adblock.so spotify 
super + y
      ytfzf -D --sort --sort-by=relevance
super + shift + y
      ytfzf -Dc S --sort
alt + p
      maim -u  ~/Pictures/ss/$(date +"%Y_%m_%d_%I_%M_%S_%p").png
alt + shift + p
      ssmaim
super + alt +p
      maim -u --select | xclip -selection clipboard -t image/png
super + period
      splatmoji type
super + shift + period
      splatmoji copy
super + z
      zathura 
super + c
      clipmenu
super + alt + Escape
     	pkill -USR1 -x sxhkd
