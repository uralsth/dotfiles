# Basic binds
super + d
      dmenu_run -p 'Run:'
super + p
      mytools
super + r
      dm-websearch
super + q
      firefox
super + shift + f
      firefox -private-window
super + control + q
      qutebrowser
super + e
      emacsclient -c -a '/usr/bin/emacs'
super + s
      spotify
super + shift + y
      ytfzf -D 
super + w 
      switch
super + alt +p
      maim -u --select | xclip -selection clipboard -t image/png
alt + p
      maim -u ~/Pictures/ss/$(date +%s).png
super + shift + alt + c
      ctoc
super + period
      splatmoji type
super + shift + period
      splatmoji copy
super + z
      zathura 
super + c
      clipmenu
