# Basic binds
super + d
      dmenu_run -p 'Run:'
super + q
      firefox
super + e
      emacsclient -c -a '/usr/bin/emacs'
super + s
      LD_PRELOAD=/usr/local/lib/spotify-adblock.so spotify
super + shift + y
      ytfzf -D 
super + w 
      switch
super + p 
      bitwarden-dmenu
super + alt +p
      maim --select | xclip -selection clipboard -t image/png
alt + p
       maim ~/Pictures/ss/$(date +%s).png
super + shift + alt + h
	mhibernate
